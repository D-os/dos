defines = -DNDEBUG -DSKIA_DLL -DSK_R32_SHIFT=16 -DSK_ENABLE_SKSL -DSK_ENABLE_PRECOMPILE -DSK_USE_PERFETTO -DSK_GAMMA_APPLY_TO_A8 -DSKIA_IMPLEMENTATION=1 -DSK_HAS_WUFFS_LIBRARY
framework_dirs =
includes = -Iexternal/skia -Ibuild/skia
cflags = $defines -Wno-attributes -ffp-contract=off -fstrict-aliasing -fPIC -fvisibility=hidden -O3 -fdata-sections -ffunction-sections -isystem /home/smoku/devel/Dos/skia/third_party/externals/wuffs/release/c
cxxflags = $defines -std=c++17 -fvisibility-inlines-hidden -fno-exceptions -fno-rtti
root_out_dir = .
target_output_name = wuffs

build $BUILDROOT/skia/codec/wuffs.SkWuffsCodec.o: cxx external/skia/src/codec/SkWuffsCodec.cpp
