defines = -DNDEBUG -DSKIA_DLL -DSK_R32_SHIFT=16 -DSK_ENABLE_SKSL -DSK_ENABLE_PRECOMPILE -DSK_USE_PERFETTO -DSK_GAMMA_APPLY_TO_A8 -DSKIA_IMPLEMENTATION=1
includes = -Iexternal/skia -Iexternal/freetype/include
cflags = $defines -Wno-attributes -ffp-contract=off -fstrict-aliasing -fPIC -fvisibility=hidden -O3 -fdata-sections -ffunction-sections
cxxflags = $defines -std=c++17 -fvisibility-inlines-hidden -fno-exceptions -fno-rtti

build $BUILDROOT/skia/ports/typeface_freetype.SkFontHost_FreeType.o: cxx external/skia/src/ports/SkFontHost_FreeType.cpp
build $BUILDROOT/skia/ports/typeface_freetype.SkFontHost_FreeType_common.o: cxx external/skia/src/ports/SkFontHost_FreeType_common.cpp
