headerDir = $BUILDROOT/binder/gen/include
imports = -Iframeworks/native/libs/binder/aidl
lang = cpp
optionalFlags = -Weverything -Wno-missing-permission-annotation
outDir = $BUILDROOT/binder/gen

build $BUILDROOT/binder/gen/android/os/IClientCallback.cpp $
| $BUILDROOT/binder/gen/include/android/os/IClientCallback.h $
  $BUILDROOT/binder/gen/include/android/os/BpClientCallback.h $
  $BUILDROOT/binder/gen/include/android/os/BnClientCallback.h: aidl $
  frameworks/native/libs/binder/aidl/android/os/IClientCallback.aidl $
| $HOSTAIDL

build $BUILDROOT/binder/gen/android/os/IServiceCallback.cpp $
| $BUILDROOT/binder/gen/include/android/os/IServiceCallback.h $
  $BUILDROOT/binder/gen/include/android/os/BpServiceCallback.h $
  $BUILDROOT/binder/gen/include/android/os/BnServiceCallback.h: aidl $
  frameworks/native/libs/binder/aidl/android/os/IServiceCallback.aidl $
| $HOSTAIDL

build $BUILDROOT/binder/gen/android/os/ServiceDebugInfo.cpp $
| $BUILDROOT/binder/gen/include/android/os/ServiceDebugInfo.h $
  $BUILDROOT/binder/gen/include/android/os/BpServiceDebugInfo.h $
  $BUILDROOT/binder/gen/include/android/os/BnServiceDebugInfo.h: aidl $
  frameworks/native/libs/binder/aidl/android/os/ServiceDebugInfo.aidl $
| $HOSTAIDL

build $BUILDROOT/binder/gen/android/os/IServiceManager.cpp $
| $BUILDROOT/binder/gen/include/android/os/IServiceManager.h $
  $BUILDROOT/binder/gen/include/android/os/BpServiceManager.h $
  $BUILDROOT/binder/gen/include/android/os/BnServiceManager.h: aidl $
  frameworks/native/libs/binder/aidl/android/os/IServiceManager.aidl $
| $BUILDROOT/binder/gen/include/android/os/IClientCallback.h $
  $BUILDROOT/binder/gen/include/android/os/IServiceCallback.h $
  $BUILDROOT/binder/gen/include/android/os/ServiceDebugInfo.h $
  $HOSTAIDL
